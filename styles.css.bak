body{
	background-color: rgb(0, 99, 158)!important;
}
.mainBody{
	background-color: white;
}
.container{
	width:100%;
	padding-right: 0px;
	padding-left: 0px;
}
.col-md-3{
	margin-top: 10px;
}
.titleBox{
	color: #fff!important;
    height: 100px;
    width: 100px;
    font-weight: bold;
    text-align: center;
    padding-top: 33px!important;
    padding: 0px;
    font-size: 20px;
    cursor: pointer;
	float: right;
    background-color: rgb(0, 99, 158)!important;
}
.well, .well-openMotsCase{
	border-radius: 35%!important;
	color: #fff!important;
	height: 140px;
	width: 150px;
	font-weight: bold;
	text-align: center;
	background-color: rgb(176, 64, 38)!important;
	padding-top: 55px!important;
	padding: 0px;
	font-size: 20px;
	cursor: pointer;
	box-shadow: 0 3px 10px rgb(0 0 0 / 58%);
}
.arrowImage{
	cursor: pointer;
}
.well-openMotsCase{
	border-radius: 15%!important;
	height: 70px;
	width: 200px;
	margin: auto;
    margin-top: 30px;
    margin-bottom: 30px;
	padding-top: 20px!important;
}
#assets-copy-image{
	width: 35px;
    margin-top: 78px;
    float: left;
    margin-left: 20px;
}
.headerImage{
	height: 100px;
    margin-left: -15px;
    float: right;
}
.headerText{
	float:left;
	color:white;
	font-size: 60px;
}
.textBody, #lvl_1, #lvl_2, #lvl_3, #lvl_4{
	width: 90%;
    padding-left: 23%;
    text-align: left;
}
.titleTextBody{
    text-align: left;
	margin-top: 5%;
}
#textResultsDisplay{
border: 1px black solid;
    width: 30%;
	left: 35%;
    margin: auto;
	
}
#displayText{
	width: 30%;
}
#resultsImage{
	float: right;
}
#q1, #q2, #q3, #q4, #resultText{
	text-align: center;
	margin-right: 200px;
}
#level1, #level2, #level3, #level4{
	margin-left: 50px;
}
#twoblocks{
	padding-left: 72px;
}
@media (min-width: 992px){
	.col-md-3 {
		width: 20%;
	}
}
@media (max-width: 1000px) {
	.well{
		float: none;
		margin: 0 auto;
		margin-bottom: 10px;
		margin-top: 10px;
	}
	.headerImage{
		float:left;
		padding-left: 25%;
	}
	.headerText{
		float:right;
		padding-right: 5%;
	}
	#twoblocks{
		padding-left: 15px;
	}
}
@media (max-width: 700px) {
	.headerImage{
		float: none;
		margin: 0 auto;
		padding-left: 0%;
	}
	.headerText{
		float: none;
		margin: 0 auto;
		padding-right: 0%
	}
}